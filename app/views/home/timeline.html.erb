<div id="leftBar"></div>
<div id="rightBar">
  <div id="year"></div>
  <div id="share_link">
    <a href="/share/<%= @user.share_key %>">Share yout timeline to facebook</a>
  </div>
  <div id="caption-outer">
    <div id="quotes-left" class="quotes">“</div>
    <p id="caption"></p>
    <div id="quotes-right" class="quotes">”</div>
  </div>
  <div id="media-part">
    <div id="clock"></div>
    <audio autoplay="autoplay" controls autoplay loop>
      <source src=<%= "/song.mp3"%> type="audio/mpeg">
    </audio>
  <div id="map-canvas"></div>
</div>

<%= javascript_include_tag :timeline %>
